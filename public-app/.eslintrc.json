{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:vue/essential", "standard"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "globals": {
    "go": "readonly" // 自定义全局变量 myGlobalVar，只读
  },
  "rules": {
    "no-unused-vars": "off",
    "no-var": "off",
    "no-redeclare": "off",
    "no-undef": "off",
    "vue/multi-word-component-names": "off"
  }
}
